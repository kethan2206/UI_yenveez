        app:srcCompat="@drawable/coupon_card_orange" />

    <ImageView
        android:id="@+id/couponOrangePath"
        android:layout_width="92dp"
        android:layout_height="86dp"
        app:srcCompat="@drawable/couponorangepath"
        app:layout_constraintTop_toTopOf="@+id/orangeCard"
        app:layout_constraintBottom_toBottomOf="@+id/orangeCard"
        app:layout_constraintStart_toStartOf="@+id/orangeCard"
        app:layout_constraintEnd_toEndOf="@+id/orangeCard" />


</androidx.constraintlayout.widget.ConstraintLayout>