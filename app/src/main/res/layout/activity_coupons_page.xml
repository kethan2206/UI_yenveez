<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/screen_background"
    tools:context=".CouponsPage">


    <ImageView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:srcCompat="@drawable/greenbackgrounficons"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"/>


    <ImageView
        android:id="@+id/blueCard"
        android:layout_width="144dp"
        android:layout_height="168dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.1"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.100000024"
        app:srcCompat="@drawable/coupon_card_blue" />

    <ImageView
        android:id="@+id/couponBluePath"
        android:layout_width="92dp"
        android:layout_height="86dp"
        app:srcCompat="@drawable/couponbluepath"
        app:layout_constraintTop_toTopOf="@+id/blueCard"
        app:layout_constraintBottom_toBottomOf="@+id/blueCard"
        app:layout_constraintStart_toStartOf="@+id/blueCard"
        app:layout_constraintEnd_toEndOf="@+id/blueCard" />

    <ImageView
        android:id="@+id/greenCard"
        android:layout_width="144dp"
        android:layout_height="168dp"
        app:layout_constraintBottom_toBottomOf="parent"
